<!-- display all album tracks with preview  -->

<ul class="list-group list-group-flush">
  <li
    class="list-group-item d-flex justify-content-between align-items-center mx-5"
  >
    <h2>Title</h2>
    <h2>Listen</h2>
  </li>
  <% data.body.items.forEach(title => { %>
  <li
    class="list-group-item d-flex justify-content-between align-items-center mx-5"
  >
    <div><%- title.name %></div>
    <div>
      <audio controls>
        <source src="<%- title.preview_url %>" type="audio/mpeg" />
      </audio>
    </div>
  </li>
  <% }) %>
</ul>
